{
  "mcpServers": {
    "fs": {
      "type": "stdio",
      "command": "stdio",
      "args": [
        "npx -y @modelcontextprotocol/server-filesystem ~"
      ],
      "env": {}
    },
    "github": {
      "type": "stdio",
      "command": "stdio",
      "args": [
        "npx -y @modelcontextprotocol/server-github"
      ],
      "env": {}
    },
    "web": {
      "type": "stdio",
      "command": "stdio",
      "args": [
        "npx -y mcp-web-search"
      ],
      "env": {}
    },
    "wiki": {
      "type": "stdio",
      "command": "stdio",
      "args": [
        "npx -y wikipedia-mcp"
      ],
      "env": {}
    },
    "vscode": {
      "type": "stdio",
      "command": "sse",
      "args": [
        "http://localhost:3000/mcp"
      ],
      "env": {}
    },
    "python_analysis": {
      "type": "stdio",
      "command": "stdio",
      "args": [
        "python3 scripts/python-mcp-server.py"
      ],
      "env": {}
    }
  }
}
